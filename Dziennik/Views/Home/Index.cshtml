@model IEnumerable<Dziennik.Models.Ogloszenie>

@{
    ViewBag.Title = "Home Page";
}
<h2>Ogłoszenia</h2>
@foreach (var item in Model)
{
<div class="post">
    <div class="post-meta">
        <div class="row">

            <!--Heading-->
            <div class="post-title">
                <h2> @Html.DisplayFor(modelItem => item.naglowek)</h2>
            </div>

        </div>
    </div>

    <!--Short Description-->
    <div class="post-body">
        @Html.DisplayFor(modelItem => item.tresc)
    </div>
    <div class="post-meta">
        @Html.DisplayFor(modelItem => item.Nauczyciel.FullName)
        @Html.DisplayFor(modelItem => item.data)

    </div>

</div>
}